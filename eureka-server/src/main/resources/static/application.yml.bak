server:
  port: 1111
#配置应用的名字
spring:
  application:
    name: eureka-server
    
    
    
eureka:
  client: 
    register-with-eureka: true #是否将当前应用注册到注册中心
    fetch-registry: false #当前项目是否到注册中心中拉去服务列表
    service-url:   #给Map集合属性中赋值
      defaultZone: http://localhost:1111/eureka,http://localhost:2222/eureka 
      
      
      


